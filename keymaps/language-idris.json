{
  "atom-text-editor[data-grammar~=\"blodwen\"]": {
    "ctrl-alt-c": "language-blodwen:case-split",
    "ctrl-alt-t": "language-blodwen:type-of",
    "ctrl-alt-a": "language-blodwen:add-clause",
    "ctrl-alt-s": "language-blodwen:proof-search",
    "ctrl-alt-d": "language-blodwen:docs-for",
    "ctrl-alt-enter": "language-blodwen:open-repl",
    "ctrl-alt-w": "language-blodwen:make-with",
    "ctrl-alt-l": "language-blodwen:make-lemma",
    "ctrl-alt-r": "language-blodwen:typecheck",
    "ctrl-alt-m": "language-blodwen:make-case",
