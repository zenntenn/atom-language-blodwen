    "ctrl-cmd-enter": "language-blodwen:open-repl",
    "ctrl-cmd-w": "language-blodwen:make-with",
    "ctrl-cmd-l": "language-blodwen:make-lemma",
    "ctrl-cmd-r": "language-blodwen:typecheck",
    "ctrl-cmd-m": "language-blodwen:make-case",
    "ctrl-cmd-p": "language-blodwen:add-proof-clause",
    "ctrl-cmd-b": "language-blodwen:browse-namespace",
    "escape": "language-blodwen:close-information-view"
  }
}
